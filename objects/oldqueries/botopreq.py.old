class UserObject(dict):
    def __init__(s):
        object = {
            "type": "query",
            "id": "BOTOPREQ",
            "collections": ["BO"],
            "filterable": True,
            "userfunctions": ['bocoeff'],
            "request": "select timestamp, report||' - '||event_id||' (duration: '||duration||')' label, sum(executecount * 1.0) / bocoeff() value from BO group by timestamp, label order by timestamp"
        }
        super(UserObject, s).__init__(**object)
