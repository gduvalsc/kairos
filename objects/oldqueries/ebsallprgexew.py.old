class UserObject(dict):
    def __init__(s):
        if "EBSPRGEXEW" not in kairos: kairos['EBSPRGEXEW']=''
        object = {
            "type": "query",
            "id": "EBSALLPRGEXEW",
            "collections": ["EBS12CM"],
            "nocache": True,
            "userfunctions": ['ebscoeff'],
            "request": "select timestamp, 'Waiting executions' label, sum(waitcount * 1.0) / ebscoeff() value from EBS12CM where prg_name = '" + kairos["EBSPRGEXEW"] + "' group by timestamp, label order by timestamp"
        }
        super(UserObject, s).__init__(**object)
